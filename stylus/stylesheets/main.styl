round(radius, padding)
  -webkit-border-radius: radius
  -moz-border-radius: radius
  border-radius: radius
  padding: padding
  
bg()
  background-color arguments

body
  font 12px/1.3 Helvetica, Arial, sans-serif
  background white
  color #333
  
div
  round 0.5em 10px
  bg lightblue 
  margin 10px
  
ul#messages
  round 0 10px
  bg white 
  margin 10px
  
li
  list-style none
